<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainFrameController">
   <children>
      <AnchorPane prefHeight="80.0">
         <children>
            <Button fx:id="previousButton" layoutX="0.0" layoutY="10.0" mnemonicParsing="false" onAction="#onPreviousButtonAction" prefHeight="60.0" prefWidth="60.0" text="←" AnchorPane.leftAnchor="-3.0" />
            <Button fx:id="nextButton" layoutX="543.0" layoutY="10.0" mnemonicParsing="false" onAction="#onNextButtonAction" prefHeight="60.0" prefWidth="60.0" text="→" AnchorPane.rightAnchor="-3.0" />
            <Label fx:id="titleLabel" alignment="TOP_CENTER" layoutX="180.0" prefHeight="30.0" prefWidth="240.0" text="表示中のフォント" AnchorPane.topAnchor="0.0" />
            <TextField fx:id="fontFamilyTextField" editable="false" layoutX="100.0" layoutY="30.0" prefHeight="40.0" prefWidth="400.0" text="sample" AnchorPane.bottomAnchor="0.0" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </AnchorPane>
      <ScrollPane prefHeight="300.0" prefWidth="640.0">
         <content>
            <VBox>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
               <children>
                  <Label fx:id="sampleLabel1" text="サンプルラベル">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="sampleLabel2" text="1234567890 abcdefghijklmnopqrstuvwxyz ABCDEFGHIJKLMNOPQRSTUVWXYZ">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Button fx:id="sampleButton" mnemonicParsing="false" text="サンプルボタン">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
